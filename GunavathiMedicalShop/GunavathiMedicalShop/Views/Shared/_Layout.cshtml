<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <!-- Basic -->
    <!--<meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />-->
    <!-- Mobile Metas -->
    <!--<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />-->
    <!-- Site Metas -->
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <meta name="author" content="" />

    <title>Medion</title>

    <!-- slider stylesheet -->
    <link rel="stylesheet" type="text/css"
          href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.1.3/assets/owl.carousel.min.css" />

    <!-- font awesome style -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


    <!-- bootstrap core css -->
    <link rel="stylesheet" type="text/css" href="css/bootstrap.css" />

    <!-- fonts style -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:400,600,700|Roboto:400,700&display=swap" rel="stylesheet">

    <!-- Custom styles for this link -->
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <!-- responsive style -->
    <link href="~/Content/css/responsive.css" rel="stylesheet" />


    


</head>
<body>





    <div class="hero_area">
        <!-- header section strats -->
        <header class="header_section">
            <div class="container">
                <div class="top_contact-container">
                    <div class="tel_container">
                        <a href="">
                            <img src="~/Content/images/telephone-symbol-button.png" alt=""> Call : +90472 23456 
                        </a>
                    </div>
                    <div class="social-container">
                        <a href="">
                            <img src="~/Content/images/fb.png" alt="" class="s-1">
                        </a>
                        <a href="">
                            <img src="~/Content/images/twitter.png" alt="" class="s-2">
                        </a>
                        <a href="">
                            <img src="~/Content/images/instagram.png" alt="" class="s-3">
                        </a>
                    </div>
                </div>
            </div>
            <div class="container-fluid">
                <nav class="navbar navbar-expand-lg custom_nav-container pt-3">
                    <div class="navbar-brand">
                        <img src="~/Content/images/logo.png" alt="">
                        <span>
                          GUNAVATHI
                        </span>
                    </div>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <div class="d-flex  flex-column flex-lg-row align-items-center w-100 justify-content-between">
                            <ul class="navbar-nav  ">
                                <li class="nav-item active">

                                    @Html.ActionLink("Home", "HomePage", "IndexPages", new { area = "" }, new { @class = "nav-link" })


                                    @*<a class="nav-link" href="index.html">Home <span class="sr-only">(current)</span></a>*@
                                </li>
                                <li class="nav-item">
                                    @Html.ActionLink("About Us", "AboutPage", "IndexPages", new { area = "" }, new { @class = "nav-link" })
                                </li>
                                <li class="nav-item">
                                    @Html.ActionLink("Medicine", "Medicine", "IndexPages", new { area = "" }, new { @class = "nav-link" })

                                    @*<a class="nav-link" href="medicine.html"> Medicine </a>*@
                                </li>
                                <li class="nav-item">

                                    @Html.ActionLink("Buy", "Buying", "IndexPages", new { area = "" }, new { @class = "nav-link" })

                                    @*<a class="nav-link" href="buy.html"> Online Buy </a>*@
                                </li>
                                <li class="nav-item">
                                    @Html.ActionLink("Contact", "ContactPage", "IndexPages", new { area = "" }, new { @class = "nav-link" })

                                    @*<a class="nav-link" href="contact.html">Contact us</a>*@
                                </li>




                                <li class="nav-item">
                                    @Html.ActionLink("REGISTER", "Register", "IndexPages", new { area = "" }, new { @class = "nav-link" })

                                    @*<a class="nav-link" href="contact.html">Contact us</a>*@
                                </li>




                            </ul>
                            <div class="login_btn-contanier ml-0 ml-lg-5">
                                <a href="">
                                    <img src="~/Content/images/user.png" alt="">
                                    <span>
                                        @Html.ActionLink("LOGIN", "LoginUser", "Register", new { area = "" }, new { @class = "nav-link" })
                                    </span>
                                </a>
                            </div>
                        </div>
                    </div>

                </nav>
            </div>
        </header>
        <!-- end header section -->

    </div>



    @RenderBody()


    <!-- info section -->

    <section class="info_section layout_padding2 mt-5">
        <div class="container">

            <div class="row">


                <div class="col-md-6">
                    <div class="info_news">
                        <div class="navbar-brand">
                            <img src="~/Content/images/logo.png" alt="">
                            <span>
                                GUNAVATHI
                            </span>
                        </div>


                        <div class="d-flex justify-content-center justify-content-end mt-3">
                            <p>
                                Welcome to India’s leading digital Healthcare Platform.
                                We focus on Simplifying healthcare & Impacting lives!





                            </p>





                        </div>

                    </div>
                </div>







                <div class="col-md-3">
                    <div class="info_menu">
                        <h4>
                            Menu
                        </h4>
                        <ul class="navbar-nav  ">
                            <li class="nav-item active">
                                @Html.ActionLink("Home", "HomePage", "IndexPages", new { area = "" }, new { @class = "nav-link" })
                            </li>
                            <li class="nav-item">
                                @Html.ActionLink("About Us", "AboutPage", "IndexPages", new { area = "" }, new { @class = "nav-link" })
                            </li>
                            <li class="nav-item">
                                @Html.ActionLink("Medicine", "Medicine", "IndexPages", new { area = "" }, new { @class = "nav-link" })
                            </li>
                            <li class="nav-item">
                                @Html.ActionLink("Buy", "Buying", "IndexPages", new { area = "" }, new { @class = "nav-link" })
                            </li>
                        </ul>
                    </div>
                </div>












                <div class="col-md-3 mt-5">
                    <div class="info_contact">


                        <h4>
                            Contact
                        </h4>
                        <div class="box">
                            <div class="img-box">
                                <img src="~/Content/images/telephone-symbol-button.png" alt="">
                            </div>
                            <div class="detail-box">
                                <h6>
                                    +90472 23456
                                </h6>
                            </div>
                        </div>
                        <div class="box">
                            <div class="img-box">
                                <img src="~/Content/images/email.png" alt="">
                            </div>
                            <div class="detail-box">
                                <h6>
                                    gunavathimedicine@gmail
                                </h6>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>


    <!-- end info section -->
    <!-- footer section -->
    <section class="container-fluid footer_section">

        <p>
            &copy; 2024 All Rights Reserved. Design by <span style="color:chocolate">M.Manikandan</span>
        </p>
    </section>

    <!-- footer section -->

    <script type="text/javascript" src="~/Content/js/jquery-3.4.1.min.js"></script>
    <script type="text/javascript" src="~/Content/js/bootstrap.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.1/owl.carousel.min.js">
    </script>
    <script type="text/javascript">
        $(".owl-carousel").owlCarousel({
            loop: true,
            margin: 10,
            nav: true,
            navText: [],
            autoplay: true,
            responsive: {
                0: {
                    items: 1
                },
                600: {
                    items: 2
                },
                1000: {
                    items: 4
                }
            }
        });
    </script>
    <script type="text/javascript">
        $(".owl-2").owlCarousel({
            loop: true,
            margin: 10,
            nav: true,
            navText: [],
            autoplay: true,

            responsive: {
                0: {
                    items: 1
                },
                600: {
                    items: 2
                },
                1000: {
                    items: 4
                }
            }
        });
    </script>
























    <!--<div class="container body-content">

    <hr />-->
    @*<footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>*@


    <!--</div>-->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
